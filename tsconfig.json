{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@ran-one/ng.theme-ant": [
        "projects/theme-ant/src/public-api"
      ],
      "@ran-ng/theme-library/dev": [
        "projects/theme-library/src/public-api"
      ],
      "theme-zero": [
        "dist/theme-zero"
      ],
      "theme-zero/*": [
        "dist/theme-zero/*"
      ]
    }
  }
}